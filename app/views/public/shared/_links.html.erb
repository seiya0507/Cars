<%- if controller_name != 'sessions' %>
  <%= link_to "登録済みの方はこちらからログイン", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "登録がお済みでない方はこちらから新規ユーザー登録", new_registration_path(resource_name) %><br />
<% end %>